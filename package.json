{
    "name": "nyan-cat",
    "displayName": "Nyan-cat",
    "description": "nyan cat for vscode",
    "version": "0.0.1",
    "publisher": "zhengrenzhe",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./ && cp ./src/nyan-cat.js ./out/nyan-cat.js",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "file-url": "^2.0.2"
    }
}
